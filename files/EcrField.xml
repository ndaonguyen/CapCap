<?xml version="1.0" encoding="utf-8"?>
<FieldList>
  <Field Code="01" Description="Approval Code" Type="ASC" MinLen="6" MaxLen="6" Remark="Taken form NETS HOST" />
  <Field Code="02" Description="Response Text" Type="ASC" MinLen="40" MaxLen="40" Remark="Default to zeroes / spaces if values are not available" />
  <Field Code="03" Description="Transaction Date" Type="ASC" MinLen="6" MaxLen="6" Remark="YYMMDD" />
  <Field Code="04" Description="Transaction Time" Type="ASC" MinLen="6" MaxLen="6" Remark="HHMMSS" />
  <Field Code="16" Description="Terminal ID" Type="ASC" MinLen="8" MaxLen="8" Remark="" />
  <Field Code="17" Description="Authorization ID" Type="ASC" MinLen="6" MaxLen="6" Remark="" />
  <Field Code="21" Description="Host Random Number" Type="ASC" MinLen="16" MaxLen="16" Remark="Containing host random number in ASCII" />
  <Field Code="22" Description="Sequence Counter" Type="ASC" MinLen="4" MaxLen="4" Remark="Sequence Counter (Incremented)" />
  <Field Code="23" Description="SAM Random Number" Type="ASC" MinLen="12" MaxLen="12" Remark="Random number generated by Concession SAM" />
  <Field Code="24" Description="SAM Cryptogram" Type="ASC" MinLen="16" MaxLen="16" Remark="Cryptogram generated by Concession SAM" />
  <Field Code="25" Description="Key Data" Type="ASC" MinLen="20" MaxLen="20" Remark="Key Data returned from the SAM. " />
  <Field Code="26" Description="Host Cryptogram" Type="ASC" MinLen="16" MaxLen="16" Remark="Host Cryptogram" />
  <Field Code="27" Description="C-MAC" Type="ASC" MinLen="16" MaxLen="16" Remark="C-MAC" />
  <Field Code="28" Description="Acquirer ID" Type="ASC" MinLen="2" MaxLen="2" Remark="" />
  <Field Code="29" Description="CSN" Type="ASC" MinLen="16" MaxLen="16" Remark="Chip Serial Number" />
  <Field Code="30" Description="Card Number" Type="ASC" MinLen="16" MaxLen="19" Remark="Default to zeroes if not available" />
  <Field Code="31" Description="RFU4" Type="ASC" MinLen="4" MaxLen="4" Remark="Default to zeroes" />
  <Field Code="32" Description="LTA SAM ID" Type="ASC" MinLen="20" MaxLen="20" Remark="" />
  <Field Code="33" Description="NETS MID" Type="ASC" MinLen="12" MaxLen="12" Remark="" />
  <Field Code="40" Description="Transaction Amount" Type="ASC" MinLen="12" MaxLen="12" Remark="Mandatory" />
  <Field Code="41" Description="Service Fee" Type="ASC" MinLen="12" MaxLen="12" Remark="Total Fee, Default to zeroes if not in used" />
  <Field Code="42" Description="Cash Back Amount" Type="ASC" MinLen="12" MaxLen="12" Remark="Must be all zeroes" />
  <Field Code="43" Description="Loyalty Redemption" Type="ASC" MinLen="1" MaxLen="1" Remark="'0'(30): Without Redemption, '1'(31): With Redemption" />
  <Field Code="46" Description="Fee due to merchant" Type="ASC" MinLen="12" MaxLen="12" Remark="Default to zeroes if not in used" />
  <Field Code="47" Description="Fee due from merchant" Type="ASC" MinLen="12" MaxLen="12" Remark="Default to zeroes if not in used." />
  <Field Code="48" Description="Cash Deposit Net Fee" Type="ASC" MinLen="12" MaxLen="12" Remark="Amount being deposited into bank account after fee deduction." />
  <Field Code="49" Description="Cash Deposit Fee" Type="ASC" MinLen="12" MaxLen="12" Remark="Default to space / zeroes if not available" />
  <Field Code="50" Description="Batch Number/ RFU6/ RFU12" Type="ASC" MinLen="6" MaxLen="12" Remark="YYMMDDHHMMSS or default to zeroes." />
  <Field Code="65" Description="STAN" Type="ASC" MinLen="6" MaxLen="6" Remark="Incremental Value" />
  <Field Code="9A" Description="AID (EMV)" Type="ASC" MinLen="16" MaxLen="16" Remark="Default to zeroes if not applicable" />
  <Field Code="9B" Description="Application Profile (EMV)" Type="ASC" MinLen="16" MaxLen="16" Remark="Default to zeroes if not applicable" />
  <Field Code="9C" Description="Cryptogram (EMV)" Type="ASC" MinLen="2" MaxLen="2" Remark="Default to zeroes if not applicable" />
  <Field Code="9D" Description="Transaction Certificate (EMV)" Type="ASC" MinLen="16" MaxLen="16" Remark="Default to zeroes if not applicable" />
  <Field Code="9E" Description="TVR (EMV)" Type="ASC" MinLen="10" MaxLen="10" Remark="Default to zeroes if not applicable" />
  <Field Code="9F" Description="TSI (EMV)" Type="ASC" MinLen="4" MaxLen="4" Remark="Default to zeroes if not applicable" />
  <Field Code="9G" Description="Acquirer Name" Type="ASC" MinLen="10" MaxLen="10" Remark="Default to zeroes if not applicable" />
  <Field Code="9H" Description="Invoice Number" Type="ASC" MinLen="6" MaxLen="6" Remark="Mandatory" />
  <Field Code="C0" Description="Trans Data" Type="ASC" MinLen="64" MaxLen="64" Remark="Trans Data is of CEPAS 1.0 or 2.0 format." />
  <Field Code="C1" Description="CEPAS Version" Type="ASC" MinLen="2" MaxLen="2" Remark="CEPAS 1.0 = '10', CEPAS 2.0 = '20'" />
  <Field Code="C2" Description="Expiry Date" Type="ASC" MinLen="8" MaxLen="8" Remark="DDMMYYYY" />
  <Field Code="C7" Description="Selected ATU Amount" Type="ASC" MinLen="12" MaxLen="12" Remark="" />
  <Field Code="C8" Description="Existing ATU Amount" Type="ASC" MinLen="6" MaxLen="6" Remark="Default is zeroes" />
  <Field Code="CA" Description="Contact Number" Type="ASC" MinLen="10" MaxLen="10" Remark="" />
  <Field Code="CB" Description="Allow Unblock" Type="ASC" MinLen="1" MaxLen="1" Remark="" />
  <Field Code="CG" Description="Activation code" Type="ASC" MinLen="10" MaxLen="10" Remark="Right justified with leading zeroes" />
  <Field Code="CH" Description="Customer ID, Country" Type="ASC" MinLen="3" MaxLen="3" Remark="Default is 'SGP'" />
  <Field Code="CI" Description="Customer ID, Type" Type="HEX" MinLen="1" MaxLen="1" Remark="0x01-NRIC, 0x02-FIN.  Default is 0x00" />
  <Field Code="CJ" Description="Customer ID" Type="ASC" MinLen="16" MaxLen="16" Remark="Default to spaces" />
  <Field Code="CK" Description="Credit Card Number" Type="ASC" MinLen="16" MaxLen="16" Remark="Default is 'XXXXXXXXXXXXXXXX'" />
  <Field Code="CL" Description="Insurance Flag" Type="ASC" MinLen="1" MaxLen="1" Remark="Mandatory" />
  <Field Code="CM" Description="Retailer Reference Number" Type="ASC" MinLen="6" MaxLen="6" Remark="Default to zeroes" />
  <Field Code="D0" Description="Merchant Name And Address" Type="ASC" MinLen="69" MaxLen="69" Remark="" />
  <Field Code="D1" Description="Merchant ID" Type="ASC" MinLen="15" MaxLen="15" Remark="" />
  <Field Code="D2" Description="Card Issuer Name" Type="ASC" MinLen="10" MaxLen="10" Remark="Default to spaces/zeroes if not available" />
  <Field Code="D3" Description="Retrieval Reference Number" Type="ASC" MinLen="12" MaxLen="12" Remark="Default to spaces/zeroes if not available" />
  <Field Code="D4" Description="RFU2" Type="ASC" MinLen="2" MaxLen="2" Remark="Default to zeroes" />
  <Field Code="D5" Description="Signature Required" Type="ASC" MinLen="1" MaxLen="1" Remark="'0'(30): Not required, '1'(31): Required" />
  <Field Code="D6" Description="Card Holder Name" Type="ASC" MinLen="26" MaxLen="26" Remark="Default to zeroes if not applicable" />
  <Field Code="E0" Description="Chip Card Data" Type="ASC" MinLen="64" MaxLen="64" Remark="Necessary chip data to be printed on receipt" />
  <Field Code="F0" Description="Foreign MID" Type="ASC" MinLen="15" MaxLen="15" Remark="Default to spaces / zeroes if not available" />
  <Field Code="F1" Description="Issuer Category" Type="ASC" MinLen="1" MaxLen="1" Remark="" />
  <Field Code="F2" Description="Currency Name" Type="ASC" MinLen="3" MaxLen="3" Remark="Foreign Data" />
  <Field Code="FA" Description="Foreign Amount" Type="ASC" MinLen="12" MaxLen="12" Remark="Data is unpacked from 6 bytes foreign amount message" />
  <Field Code="H4" Description="ECR Reference Number" Type="ASC" MinLen="10" MaxLen="10" Remark="Optional field" />
  <Field Code="H5" Description="Terminate Certificate" Type="ASC" MinLen="6" MaxLen="6" Remark="Default to zeroes if not available" />
  <Field Code="H6" Description="STAN/ RFU6" Type="ASC" MinLen="6" MaxLen="6" Remark="Incremental Number" />
  <Field Code="H7" Description="CashCard transaction Counter " Type="ASC" MinLen="6" MaxLen="6" Remark="Default to zeroes if not available" />
  <Field Code="H8" Description="Blacklist Version" Type="ASC" MinLen="6" MaxLen="6" Remark="" />
  <Field Code="H9" Description="Sign Certificate" Type="ASC" MinLen="4" MaxLen="4" Remark="Default to zeroes if not available" />
  <Field Code="HA" Description="Checksum" Type="ASC" MinLen="2" MaxLen="2" Remark="" />
  <Field Code="HB" Description="RFU8" Type="ASC" MinLen="8" MaxLen="8" Remark="Default to zeroes" />
  <Field Code="HC" Description="Card Balance" Type="ASC" MinLen="12" MaxLen="12" Remark="CashCard Balance" />
  <Field Code="L1" Description="Loyalty Program Name" Type="ASC" MinLen="24" MaxLen="24" Remark="Loyalty Data" />
  <Field Code="L2" Description="Loyalty Type" Type="ASC" MinLen="1" MaxLen="1" Remark="Loyalty Data" />
  <Field Code="L3" Description="Redemption Value" Type="ASC" MinLen="12" MaxLen="12" Remark="Loyalty Data" />
  <Field Code="L4" Description="Current Loyalty Balance" Type="ASC" MinLen="12" MaxLen="12" Remark="Loyalty Data" />
  <Field Code="L5" Description="POS Messages" Type="ASC" MinLen="240" MaxLen="240" Remark="" />
  <Field Code="L6" Description="Loyalty Marketing Message" Type="ASC" MinLen="72" MaxLen="72" Remark="Loyalty Data" />
  <Field Code="L7" Description="Card Name" Type="ASC" MinLen="20" MaxLen="20" Remark="" />
  <Field Code="L8" Description="Loyalty Program Exp Date" Type="ASC" MinLen="8" MaxLen="8" Remark="Loyalty Data" />
  <Field Code="MN" Description="Mobile Number" Type="ASC" MinLen="9" MaxLen="9" Remark="Set to 0x00 if skip" />
  <Field Code="R0" Description="Response Message I" Type="ASC" MinLen="20" MaxLen="20" Remark="Default to Space." />
  <Field Code="R1" Description="Response Message II" Type="ASC" MinLen="20" MaxLen="20" Remark="Default to Space." />
  <Field Code="S2" Description="ATU Status" Type="ASC" MinLen="2" MaxLen="2" Remark="00 = disable, 01 = enable, 02 = not applicable" />
  <Field Code="S3" Description="Purse Status" Type="ASC" MinLen="2" MaxLen="2" Remark="00 = disable, 01 = enable, 02 = not applicable" />
  <Field Code="S4" Description="Record Index" Type="ASC" MinLen="2" MaxLen="2" Remark="Starts from 1 to N. Each pass will have this value increment by 1." />
  <Field Code="S5" Description="Number of Record" Type="ASC" MinLen="2" MaxLen="2" Remark="Max number of records to extract. Kept constant for all passes." />
  <Field Code="S7" Description="Number of Records Read" Type="ASC" MinLen="2" MaxLen="2" Remark="Number of Records Successfully Read" />
  <Field Code="S8" Description="Transaction Logs" Type="ASC" MinLen="160" MaxLen="160" Remark="" />
  <Field Code="S9" Description="Max Transaction Log Count" Type="ASC" MinLen="4" MaxLen="4" Remark="Number of transaction log in the card" />
  <Field Code="T1" Description="Payment Option" Type="HEX" MinLen="1" MaxLen="1" Remark="Set to 01 for Cash Payment" />
  <Field Code="T2" Description="Transaction Type Indicator" Type="ASC" MinLen="2" MaxLen="2" Remark="Default is '00'" />
  <Field Code="T3" Description="Transaction Specific Data" Type="ASC" MinLen="106" MaxLen="106" Remark="Default to spaces / zeroes if values are not available" />
  <Field Code="T4" Description="ATU Debt Amount" Type="ASC" MinLen="12" MaxLen="12" Remark="" />
  <Field Code="ZA" Description="Total Fee (Top Up)" Type="ASC" MinLen="6" MaxLen="6" Remark="Default to zeroes if not in used" />
  <Field Code="ZB" Description="Fee due to merchant (Top Up)" Type="ASC" MinLen="6" MaxLen="6" Remark="Default to zeroes if not in used" />
  <Field Code="ZC" Description="Fee due from merchant (Top Up)" Type="ASC" MinLen="6" MaxLen="6" Remark="Default to zeroes if not in used" />
  <Field Code="ZP" Description="Purchase Fee" Type="ASC" MinLen="6" MaxLen="6" Remark="Fee for EFTPOS Purchase Transaction or Purchase Fee for Contactless." />
  <Field Code="ZT" Description="Additional Transaction Information" Type="ASC" MinLen="0" MaxLen="19" Remark="Default to zeroes if available" />
  <Field Code="ZY" Description="Transaction Code" Type="ASC" MinLen="2" MaxLen="2" Remark="Transaction Code" />
</FieldList>